def stage4():
    inputs = [
        (1, -3, 2),
        (1, 2, 1),
        (1, 0, -4)
    ]
    for a, b, c in inputs:
        result = quadratic_solution(a, b, c)
        print(f"Stage 4 (Multiple Inputs - {a}, {b}, {c}):", result)

if __name__ == "__main__":
    stage1()
    stage2()
    stage3()
    stage4()
